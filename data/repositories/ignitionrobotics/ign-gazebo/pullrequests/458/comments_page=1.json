gazebo/pullrequests/458/comments/125661662.json"}, "html": {"href": "#!/ignitionrobotics/ign-gazebo/pull-requests/458/_/diff#comment-125661662"}}, "depth": 1}, "deleted": false, "pullrequest": {"type": "pullrequest", "id": 458, "links": {"self": {"href": "data/repositories/ignitionrobotics/ign-gazebo/pullrequests/458.json"}, "html": {"href": "#!/ignitionrobotics/ign-gazebo/pull-requests/458"}}, "title": "Add option to right click and remove nodes"}, "content": {"raw": "Yup, also worth mentioning that the issue is not caused by this PR, since it should still happen if deleting the followed model by directly calling the remove service, for example.", "markup": "markdown", "html": "<p>Yup, also worth mentioning that the issue is not caused by this PR, since it should still happen if deleting the followed model by directly calling the remove service, for example.</p>", "type": "rendered"}, "created_on": "2019-11-20T22:48:56.330762+00:00", "user": {"display_name": "Louise Poubel", "uuid": "{5cfa2075-477b-4ded-bdb9-8d2479544ec4}", "links": {"self": {"href": "https://api.bitbucket.org/2.0/users/%7B5cfa2075-477b-4ded-bdb9-8d2479544ec4%7D"}, "html": {"href": "https://bitbucket.org/%7B5cfa2075-477b-4ded-bdb9-8d2479544ec4%7D/"}, "avatar": {"href": "https://avatar-management--avatars.us-west-2.prod.public.atl-paas.net/557058:6ff86fcb-b7ab-44a5-b8a6-f6d9cae8b6e8/7d903d90-c5ea-4182-b7ef-0d467e9e1c74/128"}}, "nickname": "chapulina", "type": "user", "account_id": "557058:6ff86fcb-b7ab-44a5-b8a6-f6d9cae8b6e8"}, "updated_on": "2019-11-20T22:48:56.338051+00:00", "type": "pullrequest_comment", "id": 125678235}, {"links": {"self": {"href": "data/repositories/ignitionrobotics/ign-gazebo/pullrequests/458/comments/125857416.json"}, "code": {"href": "https://api.bitbucket.org/2.0/repositories/ignitionrobotics/ign-gazebo/diff/ignitionrobotics/ign-gazebo:a8aa1e2f7238..937f186fd5fc?path=include%2Fignition%2Fgazebo%2FConversions.hh"}, "html": {"href": "#!/ignitionrobotics/ign-gazebo/pull-reques